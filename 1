#include <iostream>
#include <chrono>
#include <string>
#include <vector>
#include <list>


using namespace std;
using namespace chrono;

const int liczba_iteracji = 100000;

int main() {
    vector<int> wektor;
    auto czas_start_wektor = high_resolution_clock::now();
    for (int i = 0; i < liczba_iteracji; ++i) wektor.insert(wektor.begin(), 123456);
    auto czas_koniec_wektor = high_resolution_clock::now();
    cout << "Czas dla wektora: " << duration<double>(czas_koniec_wektor - czas_start_wektor).count() << " s\n";

    list<int> lista;
    auto czas_start_lista = high_resolution_clock::now();
    for (int i = 0; i < liczba_iteracji; ++i) lista.insert(lista.begin(), 123456);
    auto czas_koniec_lista = high_resolution_clock::now();
    cout << "Czas dla listy: " << duration<double>(czas_koniec_lista - czas_start_lista).count() << " s\n";

    return 0;
}

